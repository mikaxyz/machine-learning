{
  "private": true,
  "name": "xyz-mika-machine-learning",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "extract-mnist-data": "mkdir -p .data && tar -xf data/mnist_train.csv.tgz -C .data && tar -xf data/mnist_test.csv.tgz -C .data",
    "build": "./src/XYZMika/Debug/run.sh && elm make src/Main.elm --output dist/main.js --optimize",
    "build:dev": "./src/XYZMika/Debug/run.sh debug && elm make src/Main.elm --output dist/main.js",
    "test": "elm-test",
    "train": "./index.js train --data-path=.data/mnist_train.csv --data-limit=100 --neurons=10",
    "test-model": "./index.js test --model-path=.models/lr=0.01,af=tanh,in=784,out=10,l=[10],i=100.mnist_train_csv.json --data-path=.data/mnist_test.csv --data-limit=10000"
  },
  "author": "Mika Jauhonen",
  "license": "ISC",
  "devDependencies": {
    "elm": "^0.19.1-5",
    "elm-format": "^0.8.5",
    "elm-test": "^0.19.1-revision7",
    "yargs": "^17.7.2"
  },
  "dependencies": {
    "@andrewmacmurray/elm-concurrent-task": "^1.1.0"
  }
}
